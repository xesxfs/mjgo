git add
git commit -m ""
git status
git log
git reset --hard HARD
git reflog
git checkout -- file
git reset HEAD file
git rm file
git branch 						//查看分支
git branch <name>				//创建分支
git checkout <name>				//切换分支
git checkout -b <name>			//创建并切换分支
git merge <name>				//合并分支
git branch -d <name>          //删除分支
git merge <name>             //合并分支